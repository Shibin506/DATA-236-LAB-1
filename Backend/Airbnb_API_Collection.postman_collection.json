      "item": [
        {
          "name": "Register Traveler",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"name\": \"Test Traveler\",\n  \"email\": \"traveler@example.com\",\n  \"password\": \"password123\",\n  \"user_type\": \"traveler\"\n}"
            },
            "url": {
              "raw": "{{base_url}}/api/auth/register",
              "host": ["{{base_url}}"],
              "path": ["api", "auth", "register"]
            }
          },
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    const cookies = pm.response.headers.get('Set-Cookie');",
                  "    if (cookies) {",
                  "        const sessionMatch = cookies.match(/airbnb\\.session=([^;]+)/);",
                  "        if (sessionMatch) {",
                  "            pm.environment.set('session_cookie', sessionMatch[1]);",
                  "        }",
                  "    }",
                  "}"
                ]
              }
            }
          ]
        },
